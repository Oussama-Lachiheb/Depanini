<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #FFFAF0;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="depaninidhia.gui.ReclamationAdminController">
   <children>
      <Pane prefHeight="400.0" prefWidth="200.0" style="-fx-background-color: #FF4500;">
         <children>
            <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="25.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../../../../Desktop/depp.png" />
               </image>
            </ImageView>
         </children>
      </Pane>
      <TableView fx:id="tablereclamationadmin" layoutX="227.0" layoutY="70.0" onMouseClicked="#selected_reclamation" prefHeight="200.0" prefWidth="357.0">
        <columns>
          <TableColumn fx:id="idreclamation" prefWidth="90.0" text="Id Reclamation" />
          <TableColumn fx:id="typereclamation" prefWidth="113.0" text="Service" />
            <TableColumn fx:id="descriptionreclamation" prefWidth="153.0" text="Description Reclamation" />
        </columns>
      </TableView>
      <Button fx:id="affreclamation" layoutX="380.0" layoutY="303.0" mnemonicParsing="false" onAction="#affrtableclamation" style="-fx-background-color: #FF4500;" text="Show" textFill="WHITE">
         <font>
            <Font name="Segoe UI" size="13.0" />
         </font></Button>
      <Button fx:id="serch_rec" layoutX="478.0" layoutY="303.0" mnemonicParsing="false" onAction="#search_rec_dhia" style="-fx-background-color: #FF4500;" text="Search" textFill="WHITE">
         <font>
            <Font name="Segoe UI" size="13.0" />
         </font>
      </Button>
      <Button fx:id="deleteclamation" layoutX="276.0" layoutY="303.0" mnemonicParsing="false" onAction="#dell_rec_dhia" style="-fx-background-color: #FF4500;" text="Delete " textFill="WHITE">
         <font>
            <Font name="Segoe UI" size="13.0" />
         </font>
      </Button>
      <TextField fx:id="id_search_dhia" layoutX="473.0" layoutY="338.0" prefHeight="25.0" prefWidth="68.0" />
   </children>
</AnchorPane>
